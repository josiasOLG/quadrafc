{
  "version": 2,
  "name": "quadrafc-admin",
  "framework": "angular",
  "outputDirectory": "dist/admin",
  "buildCommand": "ng build --configuration=production",
  "installCommand": "npm ci",
  "devCommand": "ng serve --port $PORT",
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
